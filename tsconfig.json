{
  "compilerOptions": {
    "allowJs": true,
    "outDir": "dist",
    "target": "es2016",
    "declaration": false,
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "lib": ["DOM", "ES2022"]
  }
}
