  "version": "1.0.0",
  "description": "My webpack project",
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run eslint && npm run stylelint"
    }
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "dev": "parcel --no-cache ./src/index.html -p 3000",
